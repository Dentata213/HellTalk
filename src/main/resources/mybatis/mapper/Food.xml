<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="mybatis.mapper.Food">
	<insert id="foodInsert" parameterType="Map">
		INSERT INTO food VALUES(SEQ_Food.CURRVAL,#{food_name},#{food_maker},#{food_size},#{food_kcal},#{food_tan},#{food_dan},#{food_fat},#{food_col},#{food_na});
	</insert>



</mapper>